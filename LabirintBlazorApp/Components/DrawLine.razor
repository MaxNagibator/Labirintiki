<div style="left: @(X1)px; top: @(Y1+1)px; width: @(X2-X1 + 3)px; height: @(Y2-Y1 + 3)px" class="bolt"></div>
@code {
    [Parameter]
    public int X1 { get; set; }
    [Parameter]
    public int X2 { get; set; }
    [Parameter]
    public int Y1 { get; set; }
    [Parameter]
    public int Y2 { get; set; }

}
